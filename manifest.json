{
    "manifest_version": 2,
    "name": "Dictionary",
    "version": "1.0",

    "description": "Pop-up dictionary using Merriam-Webster",

    "browser_action": {
        "default_icon": "images/mw_logo.png",
        "default_title": "Dictionary",
        "default_popup": "popup.html"
    },

    "permissions": [
        "http://www.dictionaryapi.com/"
    ]
}
